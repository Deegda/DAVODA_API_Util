{
    "name": "@deegda/davoda-util",
    "version": "0.0.0-development",
    "description": "DAVODA Util",
    "author": "",
    "license": "MIT",
    "main": "./main.min.js",
    "typings": "./index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/Deegda/DAVODA_API_Util.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com",
        "tag": "latest"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "semantic-release": "semantic-release",
        "packaging": "webpack && dts-bundle-generator --no-check -o dist/index.d.ts dist/index.d.ts && rimraf dist/src dist/index.js dist/index.js.map dist/tsconfig.build.tsbuildinfo"
    },
    "dependencies": {
        "@aws-sdk/client-ses": "^3.43.0",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^1.1.5",
        "@nestjs/core": "^8.0.0",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/typeorm": "^8.0.2",
        "@types/nodemailer": "^6.4.4",
        "aws-sdk": "^2.1046.0",
        "bcrypt": "^5.0.1",
        "cache-manager": "^3.6.0",
        "nodemailer": "^6.7.2",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.0.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.2.3",
        "@semantic-release/exec": "^6.0.2",
        "@semantic-release/github": "^8.0.2",
        "@semantic-release/npm": "^8.0.3",
        "@types/bcrypt": "^5.0.0",
        "@types/cache-manager": "^3.4.2",
        "@types/express": "^4.17.13",
        "@types/jest": "27.0.2",
        "@types/node": "^16.0.0",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.3",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "dts-bundle-generator": "^6.4.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.2.5",
        "path": "^0.12.7",
        "prettier": "^2.3.2",
        "semantic-release": "^18.0.1",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.3.5",
        "webpack": "^5.66.0",
        "webpack-cli": "^4.9.1",
        "webpack-node-externals": "^3.0.0",
        "terser-webpack-plugin": "^5.3.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
